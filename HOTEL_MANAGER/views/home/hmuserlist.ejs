<!DOCTYPE html>
<html>
<head>
	<title>Show Post</title>
</head>
<body>
	<h1>Show Post</h1>

	<a href="/home">Back</a> | 
	<a href="/logout">logout</a> 
	<br>

	<table border="1">
		<tr>
			<td>Post ID</td>
			<td>Post Title</td>
			<td>Room Name</td>
			<td>Room Price</td>
			<td>Room Type</td>
			<td>Room Description</td>
			<td>Room Availability</td>
			<!-- <td>Tag</td> -->
			<td>Action</td>
		</tr>
	

		<% 
			users.forEach( function(std){ %>
			<tr>
				<td><%= std.pid %></td>
				<td><%= std.ptitle %></td>
				<td><%= std.rname %></td>
				<td><%= std.rprice %></td>
				<td><%= std.rtype %></td>
				<td><%= std.rdesc %></td>
				<td><%= std.avail %></td>
				<!-- <td><%= std.tag %></td> -->
				<td>
					<a href="/user/hmedit/<%= std.pid %>">Edit</a> |
					<a href="/user/hmdelete/<%= std.pid %>">Delete</a>
				</td>
			</tr>

		<%
			}); 
		%>
		
	</table>

</body>
</html>

